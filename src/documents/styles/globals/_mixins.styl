vendors = webkit moz o ms official
  
box-sizing()
  property = box-sizing
  for vendor in vendors
    if (vendor == official)
      {property}: arguments
    else
      -{vendor}-{property}: arguments

border-radius()
  for vendor in vendors
    if (vendor == official)
      border-radius: arguments
    else
      -{vendor}-border-radius: arguments

transition()
  for vendor in vendors
    if (vendor == official)
      transition: arguments
    else
      -{vendor}-transition: arguments

transform()
  for vendor in vendors
    if (vendor == official)
      transform: arguments
    else
      -{vendor}-transform: arguments

opacity()
  for vendor in vendors
    if (vendor == official)
      opacity: arguments
    else
      -{vendor}-opacity: arguments